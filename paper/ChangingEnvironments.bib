	langid = {english},
	keywords = {computers in biology, environmental science, informatics, instrumentation},
	file = {Full Text PDF:/home/emily/.zotero/zotero/9roxcqdt.default/zotero/storage/27FK68AZ/Campbell et al. - 2013 - Quantity is Nothing without Quality Automated QA.pdf:application/pdf;Snapshot:/home/emily/.zotero/zotero/9roxcqdt.default/zotero/storage/6MQFCMHN/574.html:text/html}
