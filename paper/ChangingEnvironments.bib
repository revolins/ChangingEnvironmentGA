	translator = {Brock, Sam},
	date = {2000-05-01},
	langid = {english},
	keywords = {Fiction / General, Health \& Fitness / Exercise, Literary Collections / Essays, Literary Collections / General, Literary Criticism / Reference, Sports \& Recreation / Walking}
